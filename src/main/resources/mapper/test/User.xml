<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.honda.interauto.dao.test.UserDao">
    <sql id="userInfo">
		name, age
	</sql>
    <select id="getAllUsers" resultType="com.honda.interauto.dto.UserDto">
        select <include refid="userInfo"/> from user
    </select>
</mapper>